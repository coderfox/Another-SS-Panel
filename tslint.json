{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-consistent-codestyle",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-floating-promises": true,
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "rulesDirectory": []
}
